<p>いいねした投稿一覧</p>

<% @favorite_list.each do |list| %>

  <%= link_to image_tag( list.image.variant(resize: '100x100') ), post_path(list.id) %>
  <%= link_to list.user.name, user_path(list.user.id) %>
  <%= list.favorites.length %>
  <%= link_to list.title, post_path(list.id) %>
  <%= link_to list.introduction, post_path(list.id) %>

<% end %>
